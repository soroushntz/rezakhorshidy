class Student:
    def __init__(self, name, age):
        self.__name = name # private variable
        self.__age = age # private variable

    def set_name(self, name):
        self.__name = name

    def set_age(self, age):
        self.__age = age

    def get_name(self):
        return self.__name

    def get_age(self):
        return self.__age
