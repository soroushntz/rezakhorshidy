class student:
    def __init__(self,id=1111,name='reza'):
          self.id=id
          self.name=name
    def __del__(self):
          print(self.name,'deleted')
    def  mean(self) :
         s1=int(input ())
         s2=int(input())
         sum=s1+s2
         return sum/2
    def is_mashroot(self):
         if self.mean() <12:
           return True

         else:
           return False
    def __str__(self) :
        print('this calss is student calss')
     
